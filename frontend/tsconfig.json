
{
	"allowSyntheticDefaultImports": true,
	"esModuleInterop": true,
	"compilerOptions": {
		"outDir": "./public/",
		"allowJs": true,
		"target": "es5",
		"jsx": "react",
		"lib": [
			"dom",
			"ES2021.String"
		],
		"baseUrl": ".",
		"resolveJsonModule": true,
		"esModuleInterop": true,
   	"allowSyntheticDefaultImports": true,

		"paths": {
			"@assets/*": [
				"./src/assets/*"
			],
			"@hooks/*": [
				"./src/hooks/*"
			],
			"@env":[
				"./src/config.json"
			] 

		}
	},
	"include": ["./src/**/*"]
}
